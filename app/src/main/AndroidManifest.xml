<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.today_project">

    <application
        android:allowBackup="true"
        android:icon="@drawable/today_launcher_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/today_launcher_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Today">
        <activity
            android:name=".initial.SplashScreen"
            android:theme="@style/Theme.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".AddActivity" />
        <activity android:name=".ItemActivity" />
        <activity android:name=".TimerActivity"/>
        <activity android:name=".initial.Welcome"/>
        <activity android:name=".initial.Log_in"/>
        <activity android:name=".initial.Sign_in"/>
        <activity android:name=".NavigationActivity"/>
        <activity android:name=".TimerInterruptActivity"/>
        <activity android:name=".TimerEndActivity"/>
        <activity android:name=".Notifications">
            </activity>


    </application>

</manifest>